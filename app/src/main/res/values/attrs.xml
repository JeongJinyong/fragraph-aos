<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HistoryView">
        <attr name="bg" format="reference|integer" />
        <attr name="day" format="reference|string" />
        <attr name="incense" format="reference|string" />
        <attr name="feeling" format="reference|string" />
        <attr name="playtime" format="reference|string" />
        <attr name="playtime_end" format="reference|string" />
        <attr name="tags" format="reference" />
        <attr name="memo" format="reference|string" />
    </declare-styleable>


    <declare-styleable name="BlurLayout">
        <attr name="blk_downscaleFactor" format="float" />
        <attr name="blk_blurRadius" format="integer" />
        <attr name="blk_fps" format="integer" />
        <attr name="blk_cornerRadius" format="dimension" />
        <attr name="blk_alpha" format="float" />
    </declare-styleable>

    <declare-styleable name="TextOutLineView">
        <attr name="text_stroke" format="boolean"/>
        <attr name="text_stroke_width" format="float"/>
        <attr name="text_stroke_color" format="color"/>
    </declare-styleable>
</resources>